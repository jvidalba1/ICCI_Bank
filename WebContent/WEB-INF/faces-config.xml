<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
    version="1.2">
	<converter>
		<display-name>simpleDateFormat</display-name>
		<converter-id>simpleDateFormat</converter-id>
		<converter-class>com.infy.icci.converter.SimpleDateConverter</converter-class>
	</converter>
	<managed-bean>
		<managed-bean-name>viewCardDetailsMB</managed-bean-name>
		<managed-bean-class>com.infy.icci.managedBean.ViewCardDetailsMB</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>cardMB</managed-bean-name>
		<managed-bean-class>com.infy.icci.managedBean.CardMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>revenueStatisticsMB</managed-bean-name>
		<managed-bean-class>com.infy.icci.managedBean.RevenueStatisticsMB</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>loginMB</managed-bean-name>
		<managed-bean-class>com.infy.icci.managedBean.LoginMB</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>customerMB</managed-bean-name>
		<managed-bean-class>com.infy.icci.managedBean.CustomerMB</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>blockCardMB</managed-bean-name>
		<managed-bean-class>com.infy.icci.managedBean.BlockCardMB</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>cardApplicationMB</managed-bean-name>
		<managed-bean-class>com.infy.icci.managedBean.CardApplicationMB</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>schemeReportMB</managed-bean-name>
		<managed-bean-class>com.infy.icci.managedBean.SchemeReportMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>blockStatisticsMB</managed-bean-name>
		<managed-bean-class>com.infy.icci.managedBean.BlockStatisticsMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>transactionStatisticsMB</managed-bean-name>
		<managed-bean-class>com.infy.icci.managedBean.TransactionStatisticsMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>transactionMB</managed-bean-name>
		<managed-bean-class>com.infy.icci.managedBean.TransactionMB</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>schemeMB</managed-bean-name>
		<managed-bean-class>com.infy.icci.managedBean.SchemeMB</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>paymentMB</managed-bean-name>
		<managed-bean-class>com.infy.icci.managedBean.PaymentMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>cardUsageMB</managed-bean-name>
		<managed-bean-class>com.infy.icci.managedBean.CardUsageMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>cardStatusMB</managed-bean-name>
		<managed-bean-class>com.infy.icci.managedBean.CardStatusMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>unblockCardMB</managed-bean-name>
		<managed-bean-class>com.infy.icci.managedBean.UnblockCardMB</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<navigation-rule>
		<display-name>LoginScreen</display-name>
		<from-view-id>/LoginScreen.jsp</from-view-id>
		<navigation-case>
			<from-outcome>successCr</from-outcome>
			<to-view-id>/CRHomePage.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>LoginScreen</display-name>
		<from-view-id>/LoginScreen.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/CustomerLogin.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>CustomerLogin</display-name>
		<from-view-id>/CustomerLogin.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/CardDetails.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>CardDetails</display-name>
		<from-view-id>/CardDetails.jsp</from-view-id>
		<navigation-case>
			<from-outcome>noscheme</from-outcome>
			<to-view-id>/ChooseScheme.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>CardDetails</display-name>
		<from-view-id>/CardDetails.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/CustomerHomePage.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>BlockCard</display-name>
		<from-view-id>/BlockCard.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/BlockCardSuccess.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>ApproveCard</display-name>
		<from-view-id>/ApproveCard.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/ApproveCardSuccess.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>ApplyForCard</display-name>
		<from-view-id>/ApplyForCard.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/ApplyForCard.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>SchemeReport</display-name>
		<from-view-id>/SchemeReport.jsp</from-view-id>
		<navigation-case>
			<to-view-id>/SchemeDetails.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>On-LineTransaction</display-name>
		<from-view-id>/On-LineTransaction.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/On-LineTransactionSuccess.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>ChooseScheme</display-name>
		<from-view-id>/ChooseScheme.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/SchemeAgreement.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>SchemeAgreement</display-name>
		<from-view-id>/SchemeAgreement.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/SchemeSuccess.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Payment</display-name>
		<from-view-id>/Payment.jsp</from-view-id>
		<navigation-case>
			<from-outcome>Succesfull</from-outcome>
			<to-view-id>/PaymentSuccess.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Approved&amp;Pending_RejectedCardReport</display-name>
		<from-view-id>/Approved&amp;Pending_RejectedCardReport.jsp</from-view-id>
		<navigation-case>
			<from-outcome>successfullApprove</from-outcome>
			<to-view-id>/ApprovedCardDetails.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Approved&amp;Pending_RejectedCardReport</display-name>
		<from-view-id>/Approved&amp;Pending_RejectedCardReport.jsp</from-view-id>
		<navigation-case>
			<from-outcome>successReject</from-outcome>
			<to-view-id>/Pending&amp;RejectedApplicationDetails.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>UnblockCard</display-name>
		<from-view-id>/UnblockCard.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/UnblockCardSuccess.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<validator>
		<display-name>ValidateEmail</display-name>
		<validator-id>validateEmail</validator-id>
		<validator-class>com.infy.icci.validator.ValidateEmail</validator-class>
	</validator>
	
</faces-config>
